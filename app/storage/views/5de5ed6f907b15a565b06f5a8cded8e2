<tr>
    <td><?php echo link_to("{$serviceDocument["filepath"]}/{$serviceDocument["filename"]}", str_replace('_', ' ', $serviceDocument["document"])); ?></td>
    <td><?php echo $serviceDocument["created_at"]; ?></td>
    </tr>
<?php endforeach; ?>
    <?php foreach($jobDocuments as $jobDocument): ?>
        <tr>
            <td><?php echo link_to("{$jobDocument->filepath}/{$jobDocument->filename}", str_replace('_', ' ', $jobDocument->document)); ?></td>
            <td><?php echo $jobDocument->created_at; ?></td>
        </tr>
    <?php endforeach; ?>
    </table>
<?php $__env->stopSection(); ?>

